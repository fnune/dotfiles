{
  "coc.preferences.colorSupport": false,
  "coc.preferences.jumpCommand": "tab drop",
  "coc.preferences.formatOnSaveFiletypes": [
    "Markdown",
    "css",
    "graphql",
    "javascript",
    "javascriptreact",
    "json",
    "python",
    "rust",
    "sass",
    "scss",
    "svelte",
    "typescript",
    "typescript.tsx",
    "typescriptreact"
  ],
  "prettier.disableSuccessMessage": true,
  "eslint.autoFixOnSave": true,
  "graphql.filetypes": ["graphql"]
}

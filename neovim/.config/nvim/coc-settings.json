{
  "coc.preferences.formatOnType": true,
  "coc.preferences.maxFileSize": "200MB",
  "coc.preferences.promptInput": false,
  "coc.preferences.useQuickfixForLocations": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "languageserver": { "nix": { "command": "rnix-lsp", "filetypes": ["nix"] } },
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.checkOnSave": true,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "diagnostic.hintSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "floatFactory.floatConfig": {
    "border": true,
    "rounded": true,
    "highlight": "Normal"
  },
  "diagnostic.floatConfig": { "title": "" },
  "hover.floatConfig": { "title": "" },
  "signature.floatConfig": { "title": "󰘧" },
  "suggest.floatConfig": { "title": "" },
  "colors.enable": true,
  "highlight.colorNames.enable": false
}
